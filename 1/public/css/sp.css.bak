}
.collection{
    display: block;
    padding-top: 18px;
    background: url(collect_ico.png) no-repeat top center;
    width: 28px;
    color: #a7a7a7;
    float: right;
    font-size: 12px;
    margin-top: 8px;
    text-align: center;
	text-decoration: none;
}
.collection:hover {
    background: url(collecth_ico.png) no-repeat top center;
    color: #ff9748;
}
.pro_info>div{
	 position: relative;
	 height: 40px;
	 line-height: 40px;
}
.pro_info>div>span:first-child{
	position: absolute;
}
.pro_info>div:nth-child(2)>span:last-child{
	position: absolute;
	left:80px;
	font-size: 30px;
	color: #ff9748;
}
.pro_info>div:nth-child(3)>span:last-child{
	position: absolute;
	left:80px;
	font-size: 16px;
	text-decoration:line-through;
}
.pro_info>div:nth-child(5)>span:last-child{
	position: absolute;
	left:80px;
}
